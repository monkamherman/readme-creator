{
  "name": "enatega-multivendor-orchestrator",
  "version": "1.0.0",
  "description": "Root orchestrator to launch all Enatega Multivendor projects",
  "scripts": {
    "start": "npx concurrently --kill-others-on-fail -c \"bgBlue.bold,bgMagenta.bold,bgGreen.bold,bgCyan.bold,bgYellow.bold\" \"npm:start:admin\" \"npm:start:web\" \"npm:start:app\" \"npm:start:rider\" \"npm:start:store\"",
    "start:admin": "cd enatega-multivendor-admin && npm run dev -- -p 3000",
    "start:web": "cd enatega-multivendor-web && npm run dev -- -p 3001",
    "start:app": "cd enatega-multivendor-app && npm run start",
    "start:rider": "cd enatega-multivendor-rider && npm run start",
    "start:store": "cd enatega-multivendor-store && npm run start",
    "install:all": "npm install && cd enatega-multivendor-admin && npm install && cd ../enatega-multivendor-web && npm install && cd ../enatega-multivendor-app && npm install && cd ../enatega-multivendor-rider && npm install && cd ../enatega-multivendor-store && npm install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^7.2.0",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-query": "^5.90.16",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "global": "^4.4.0",
    "graphql-ws": "^6.0.6",
    "lucide-react": "^0.562.0",
    "ngrok": "^5.0.0-beta.2",
    "prisma": "^7.2.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router-dom": "^7.11.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "tailwindcss-animate": "^1.0.7"
  }
}
